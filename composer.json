{
    "name": "affonso/laravel-auto-crud-react",
    "description": "Laravel Auto CRUD with Inertia React support - helps you streamline development and save time.",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "crud", "generator", "inertia", "react", "typescript", "shadcn"],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "laravel/prompts": "^0.1|^0.2|^0.3",
        "spatie/laravel-data": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Mrmarchone\\LaravelAutoCrud\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Abdelrahman Muhammed",
            "email": "mrmarchone@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Mrmarchone\\LaravelAutoCrud\\LaravelAutoCrudServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^3.0"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
